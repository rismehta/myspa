{
    "presets": [
        ["@babel/preset-env"
        ,
            {
                "targets": {
                    "esmodules": true
                }
            }
        ],
        "@babel/react"],
    "plugins": [
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-nullish-coalescing-operator"
    ],
    "env": {
        "test": {
            "plugins": [
                [
                    "istanbul",
                    {
                        "include": [
                            "src/**.*"
                        ],
                        "exclude": [
                            "dist/**.*",
                            "node_modules/**.*",
                            "tests/**.*"
                        ]
                    }
                ]
            ]
        }
    }
}
